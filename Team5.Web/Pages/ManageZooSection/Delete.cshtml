        <ol class="breadcrumb">
            <li class="breadcrumb-item active d-flex flex-row gap-1 align-items-center">
                <svg width="12" height="12" class="mr-4" fill="#6c757d" viewBox="0 0 20 20"
                    xmlns="http://www.w3.org/2000/svg">
                    <path
                        d="M10.707 2.293a1 1 0 00-1.414 0l-7 7a1 1 0 001.414 1.414L4 10.414V17a1 1 0 001 1h2a1 1 0 001-1v-2a1 1 0 011-1h2a1 1 0 011 1v2a1 1 0 001 1h2a1 1 0 001-1v-6.586l.293.293a1 1 0 001.414-1.414l-7-7z">
                    </path>
                </svg><span>Dashboard</span>
            </li>
            <li class="breadcrumb-item active" aria-current="page">All Section</li>
            <li class="breadcrumb-item active" aria-current="page">Delete</li>
        </ol>
    </nav>
    <div>
        <a asp-page="./Index">Back to List</a>
    </div>
    <div class="card create-form">
        <div class="card-header">
            Are you sure you want to delete this?
        </div>
        <div class="card-body">
            <h5 class="card-title mb-4">Section</h5>
            <dl class="row">
                <dt class="col-sm-5">
                    @Html.DisplayNameFor(model => model.ZooSection.Name)
                </dt>
                <dd class="col-sm-7">
                    @Html.DisplayFor(model => model.ZooSection.Name)
                </dd>
                <dt class="col-sm-5">
                    @Html.DisplayNameFor(model => model.ZooSection.Description)
                </dt>
                <dd class="col-sm-7">
                    @Html.DisplayFor(model => model.ZooSection.Description)
                </dd>
                <dt class="col-sm-5">
                    @Html.DisplayNameFor(model => model.ZooSection.ZooSectionStatus)
                </dt>
                <dd class="col-sm-7">
                    @Html.DisplayFor(model => model.ZooSection.ZooSectionStatus)
                </dd>
                <dt class="col-sm-5">
                    @Html.DisplayNameFor(model => model.ZooSection.CreatedDate)
                </dt>
                <dd class="col-sm-7">
                    @Html.DisplayFor(model => model.ZooSection.CreatedDate)
                </dd>
                <dt class="col-sm-5">
                    @Html.DisplayNameFor(model => model.ZooSection.UpdatedDate)
                </dt>
                <dd class="col-sm-7">
                    @Html.DisplayFor(model => model.ZooSection.UpdatedDate)
                </dd>

            </dl>
            <form method="post">
                <input type="hidden" asp-for="ZooSection.Id" />
                <input type="submit" value="Delete" class="btn btn-danger w-50 text-center" />

            </form>
        </div>
    </div>


</div>

