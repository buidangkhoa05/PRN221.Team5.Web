    <script>
        $(document).ready(function () {
            $('#top_animals_month').DataTable({
                "lengthMenu": [[5, 10, 15, -1], [5, 10, 15, "All"]],
                "responsive": true,
            });

        });
        $(document).ready(function () {

            $('#top_account_month').DataTable({
                "searching": false,
                "info": false,
                "lengthChange": false,
                "responsive": true,
                "paging": true,
                "pageLength": 5,
            });
        });
        $(document).ready(function () {
            $('#top_posts_month').DataTable({
                "lengthMenu": [[5, 10, 15, -1], [5, 10, 15, "All"]],
                "responsive": true,
                "columnDefs": [
                    { "orderable": false, "targets": 'no-sort' }
                ],

            });

        });

        window.addEventListener('click', function (e) {
            document.querySelectorAll('.dropdown').forEach(function (dropdown) {
                if (!dropdown.contains(e.target)) {
                    // Click was outside the dropdown, close it
                    dropdown.open = false;
                }
            });
        });

        const dropdownSummary = document.getElementById("dropdownHeader");
        const svgArrow = document.querySelector("#dropdownHeader svg");

        dropdownSummary.addEventListener("click", function () {
            // Toggle the rotation angle based on the presence of 'rotate-180' class
            const isOpen = svgArrow.classList.toggle("rotate-180");

            // Optionally, you can add logic to handle dropdown open/close behavior here
            if (isOpen) {
                // Dropdown is open, add your logic here
            } else {
                // Dropdown is closed, add your logic here
            }
        });



    </script>


